{
    "web": {
        "auth_uri":  "https://login.microsoftonline.com/{TENANT_ID}/oauth2/v2.0/authorize",
        "issuer": "https://login.microsoftonline.com/{TENANT_ID}/v2.0",
        "client_id": "{CLIENT_ID}",
        "client_secret": "{CLIENT_SECRET}",
        "redirect_uris": ["http://localhost:5000/*"],
        "token_uri": "https://login.microsoftonline.com/{TENANT_ID}/oauth2/v2.0/token",
        "userinfo_uri": "https://graph.microsoft.com/oidc/userinfo",
        "token_introspection_uri": "https://login.microsoftonline.com/{TENANT_ID}/oauth2/v2.0/token"
    }
}